{% extends '_layout/layout-frontend.html.twig' %}

{% block title %}Home{% endblock %}

{% block body %}

    <div class="container">

        <h2>{{ app.user.getUsername() }}</h2>
        <hr>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a href="{{ url('home-index') }}">{{ eryse.client.name }}</a>
                </li>
                <li class="breadcrumb-item">
                    <a href="{{ url('profile-list') }}">{% trans from 'global' %}profiles{% endtrans %}</a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                    {{ app.user.getUsername() }}
                </li>
            </ol>
        </nav>

        <div class="row">
            <p></p>
        </div>
    </div>


{% endblock %}
