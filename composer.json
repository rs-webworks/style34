{
    "type": "project",
    "license": "proprietary",
    "name": "raitocz/eryse-client",
    "description": "Eryse ACP Client Application",
    "authors": [
        {
            "name": "raitocz",
            "email": "raito.akehanareru@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "browscap/browscap-php": "^4.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "google/recaptcha": "^1.2",
        "knplabs/knp-paginator-bundle": "^4.0",
        "martinusso/opencrypt": "^3.1",
        "mashape/unirest-php": "^3.0",
        "php-translation/symfony-bundle": "^0.8.1",
        "phpseclib/phpseclib": "^2.0",
        "scheb/two-factor-bundle": "^3.16",
        "sensio/framework-extra-bundle": "^5.2",
        "squizlabs/php_codesniffer": "3.3.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "v4.4.0-RC1",
        "symfony/console": "v4.4.0-RC1",
        "symfony/debug": "v4.4.0-RC1",
        "symfony/expression-language": "v4.4.0-RC1",
        "symfony/form": "v4.4.0-RC1",
        "symfony/framework-bundle": "v4.4.0-RC1",
        "symfony/process": "v4.4.0-RC1",
        "symfony/security-bundle": "v4.4.0-RC1",
        "symfony/serializer": "v4.4.0-RC1",
        "symfony/serializer-pack": "1.0.2",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "v4.4.0-RC1",
        "symfony/twig-bundle": "v4.4.0-RC1",
        "symfony/validator": "v4.4.0-RC1",
        "symfony/web-link": "v4.4.0-RC1",
        "symfony/apache-pack": "^1.0",
        "symfony/flex": "^1.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "phpstan/phpstan": "^0.11.0",
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.2",
        "symfony/maker-bundle": "^1.9",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/profiler-pack": "1.0.4",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*"
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EryseClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EryseClient\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
