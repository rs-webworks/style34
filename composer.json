{
    "type": "library",
    "license": "proprietary",
    "name": "raitocz/eryse-client",
    "description": "Eryse ACP Client Application",
    "authors": [
        {
            "name": "raitocz",
            "email": "raito.akehanareru@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/raitocz/Tracy-Blue-Screen-Bundle"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "google/recaptcha": "^1.2",
        "knplabs/knp-paginator-bundle": "dev-master",
        "martinusso/opencrypt": "^3.1",
        "mashape/unirest-php": "^3.0",
        "phpseclib/phpseclib": "^2.0",
        "scheb/two-factor-bundle": "^3.16",
        "sensio/framework-extra-bundle": "^5.1",
        "squizlabs/php_codesniffer": "3.3.2",
        "symfony/asset": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/http-client": "5.0.*",
        "symfony/intl": "5.0.*",
        "symfony/mailer": "5.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.0.*",
        "symfony/orm-pack": "1.0.7",
        "symfony/process": "5.0.*",
        "symfony/security-bundle": "5.0.*",
        "symfony/serializer-pack": "1.0.2",
        "symfony/string": "5.0.*",
        "symfony/translation": "5.0.*",
        "symfony/twig-pack": "1.0.0",
        "symfony/validator": "5.0.*",
        "symfony/web-link": "5.0.*",
        "symfony/yaml": "5.0.*",
        "twig/cssinliner-extra": "3.x-dev",
        "twig/inky-extra": "3.x-dev",
        "twig/markdown-extra": "3.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "phpstan/phpstan": "dev-master",
        "symfony/debug-pack": "1.0.7",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "1.0.4",
        "symfony/test-pack": "1.0.6",
        "symfony/dotenv": "5.0.*",
        "symfony/phpunit-bridge": "^4.2"
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EryseClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EryseClient\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
