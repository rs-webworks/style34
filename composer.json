{
    "type": "project",
    "license": "proprietary",
    "name": "raitocz/eryse-client",
    "description": "Eryse ACP Client Application",
    "authors": [
        {
            "name": "raitocz",
            "email": "raito.akehanareru@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "ext-http": "*",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.2",
        "symfony/console": "^4.0",
        "symfony/debug": "^4.2",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "^4.2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "*",
        "symfony/security-bundle": "^4.2",
        "symfony/serializer": "^4.2",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "*",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "^4.2",

        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "php-translation/symfony-bundle": "^0.8.1",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.3",

        "phpseclib/phpseclib": "^2.0",
        "scheb/two-factor-bundle": "^3.16",
        "squizlabs/php_codesniffer": "3.3.2",
        "mashape/unirest-php": "^3.0",
        "martinusso/opencrypt": "^3.1",
        "browscap/browscap-php": "^4.1",
        "google/recaptcha": "^1.2"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.2",
        "symfony/maker-bundle": "^1.9",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*",

        "phpstan/phpstan": "^0.11.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EryseClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EryseClient\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
